<Page x:Class="DND.Barbarian"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DND"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Barbarian" Background="White">
    <Grid>
        <Grid.Background>
            <ImageBrush x:Name="Grid_Main_Background" ImageSource="https://cdn.netzpolitik.org/wp-upload/one-does-not-meme.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Button Content="Button" HorizontalAlignment="Left" Margin="363,195,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Page>
