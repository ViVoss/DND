<Window x:Class="DND.Creation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DND"
        mc:Ignorable="d"
        Title="Creation" Name="CreationName" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush x:Name="Grid_Main_Background" ImageSource="Resources\Random\maintainweb.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Button2" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="0,50,0,0" Click="Button2_Click"/>

        <Frame Name="Frame1" Margin="100,0,0,0"/>
    </Grid>
</Window>
